
{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,


  "permissions": ["storage", "activeTab", "scripting", "tabs"],


  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/Artboard3x.png"
    }
  },

  "icons": {
    "16": "assets/Artboard3x.png"
  }
}